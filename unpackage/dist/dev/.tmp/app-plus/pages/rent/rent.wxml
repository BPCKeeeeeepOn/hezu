<view class="rent-page"><view class="tab-wrap"><block wx:for="{{navList}}" wx:for-item="tab" wx:for-index="__i0__" wx:key="value"><view data-event-opts="{{[['tap',[['toggleTab',['$0'],[[['navList','value',tab.value]]]]]]]}}" class="{{[(activeTab==tab.value)?'active':'']}}" bindtap="__e">{{tab.txt}}</view></block></view><view class="goods-list"><view class="product-list"><block wx:for="{{productList}}" wx:for-item="product" wx:for-index="__i1__" wx:key="goods_id"><view data-event-opts="{{[['tap',[['toGoods',['$0'],[[['productList','goods_id',product.goods_id]]]]]]]}}" class="product" bindtap="__e"><image mode="widthFix" src="{{product.photo_min}}"></image><view class="right"><view class="name">{{product.name}}</view><view class="price">{{"房间数:"+product.roomCount}}</view><view class="slogan">{{"租房状态:"+product.roomStatus}}</view></view></view></block></view><view class="loading-text">{{loadingText}}</view></view></view>