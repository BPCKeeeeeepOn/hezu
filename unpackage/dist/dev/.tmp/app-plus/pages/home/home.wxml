<view class="home-page _div"><view class="status" style="{{'opacity:'+(afterHeaderOpacity)+';'}}"></view><view class="header"></view><view class="input-box-wraper"><text data-event-opts="{{[['tap',[['release',['$event']]]]]}}" class="release-text" bindtap="__e">我要发布</text><view class="center-input _div"><input placeholder-class="placeholder-class" placeholder="默认关键字" placeholder-style="color:#c0c0c0;" data-event-opts="{{[['tap',[['toSearch']]]]}}" bindtap="__e"/><view data-event-opts="{{[['tap',[['clearKeyword',['$event']]]]]}}" class="clear-icon" bindtap="__e">×</view></view><button class="mini-btn search-btn" type="default" size="mini">搜索</button></view><view class="tab-top-wraper"><block wx:for="{{tabBtnList}}" wx:for-item="item" wx:for-index="__i0__" wx:key="value"><view data-event-opts="{{[['tap',[['toggleTab',['$0'],[[['tabBtnList','value',item.value]]]]]]]}}" class="{{['tab-item',(activeTab==item.value)?'active':'']}}" bindtap="__e">{{''+item.text+''}}</view></block></view><view class="goods-list"><view class="product-list"><block wx:for="{{productList}}" wx:for-item="product" wx:for-index="__i1__" wx:key="goods_id"><view data-event-opts="{{[['tap',[['toGoods',['$0'],[[['productList','goods_id',product.goods_id]]]]]]]}}" class="product" bindtap="__e"><image mode="widthFix" src="{{product.photo_min}}"></image><view class="content"><view class="container"><view class="name">{{product.name}}</view><view class="info">{{product.area_order+' m²'}}</view><view class="address">{{product.subway_station_info}}</view></view></view></view></block></view><view class="loading-text">{{loadingText}}</view></view></view>