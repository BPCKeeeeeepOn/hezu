.txt-cut {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.no-more-txt {
  text-align: center;
  padding: 20rpx;
  color: #606266;
}
.rentdetail .cu-bar .content {
  height: 70rpx;
  line-height: 70rpx;
}
.rentdetail .content-title {
  font-size: 36rpx;
  padding: 46rpx 0 8rpx;
  text-align: center;
}
.rentdetail .report-btn {
  color: #999;
}
.rentdetail .cu-tag {
  padding-top: 0;
}
.rentdetail .cu-tag[class*="line-"]::after {
  border-radius: 4px;
}
.rentdetail .group-item-wraper {
  padding-bottom: 40rpx;
  border-bottom: 1px solid #e8e8e8;
}
.rentdetail .card-wraper {
  position: relative;
}
.rentdetail .card-wraper .cu-tag {
    position: absolute;
    top: 6rpx;
    right: 6rpx;
}
.rentdetail .card-wraper uni-image {
    height: 100%;
}
.rentdetail .card-wraper .bg-shadeBottom {
    position: absolute;
    bottom: 0;
    width: 100%;
    background-color: transparent;
    padding: 0px 12px;
}
.rentdetail .table-item {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  border-top: 1px solid #ccc;
  border-left: 1px solid #ccc;
}
.rentdetail .table-item text {
    padding: 16rpx 0;
    -webkit-box-flex: 1;
    -webkit-flex: 1;
            flex: 1;
    border-right: 1px solid #ccc;
    text-align: center;
}
.rentdetail .table-item .operation {
    color: #4399FC;
    text-decoration: underline;
}
.rentdetail .rent-info view {
  padding: 6rpx 0;
}
.rentdetail .rent-info view .label {
    padding-right: 12rpx;
}
.rentdetail .p-t-16 {
  padding-top: 16rpx;
}
.rentdetail .content {
  width: 92%;
  margin: 0 auto;
}
.rentdetail .label-title {
  padding-top: 26rpx;
  font-size: 32rpx;
}
.rentdetail .useringo > view {
  padding-top: 12rpx;
}
.rentdetail .useringo > view:first-child {
    padding-top: 6rpx;
}
.rentdetail .useringo .label {
  color: #333333;
  padding-right: 14rpx;
}

