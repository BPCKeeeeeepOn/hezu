<view class="rent-page"><cu-custom vue-id="1" bgColor="uni-custom-header-color" bind:__l="__l" vue-slots="{{['content']}}"><view slot="content">我的合租</view></cu-custom><view class="tab-wrap"><block wx:for="{{navList}}" wx:for-item="tab" wx:for-index="__i0__" wx:key="value"><view data-event-opts="{{[['tap',[['toggleTab',['$0'],[[['navList','value',tab.value]]]]]]]}}" class="{{[(activeTab==tab.value)?'active':'']}}" bindtap="__e">{{tab.txt}}</view></block></view><view class="goods-list"><view class="product-list"><block wx:for="{{productList}}" wx:for-item="product" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['toGoods',['$0'],[[['productList','',index,'id']]]]]]]}}" class="product" bindtap="__e"><image mode="widthFix" src="{{product.imgUrlList&&product.imgUrlList[0]?product.imgUrlList[0]+'?imageView2/1/w/800/h/434':emptyRoomPic}}"></image><view class="right"><view class="name text-cut padding-top-xs">{{product.title}}</view><view class="info">{{"租房类型: "+(product.cotenant_type==1?'预防房':'合租房')}}</view><view class="info">{{"房间数: "+product.cotenant_count}}</view><view class="info">租房状态:<block wx:for="{{statusList}}" wx:for-item="status" wx:for-index="index" wx:key="index"><block wx:if="{{product.status==status.value}}"><view class="{{['cu-tag margin-left-xs padding-left-xs padding-right-xs','line-'+status.class]}}" style="height:40rpx;">{{status.label+''}}</view></block></block></view></view></view></block></view></view><view class="no-more-txt">没有更多了哦</view></view>