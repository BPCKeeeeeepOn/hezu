<view class="chat-list-page"><cu-custom vue-id="1" bgColor="uni-custom-header-color" bind:__l="__l" vue-slots="{{['content']}}"><view slot="content">聊一聊</view></cu-custom><view class="cu-list menu-avatar"><block wx:if="{{!chatList||!chatList.length}}"><view class="no-more-txt empty-page">暂时没有消息</view></block><block wx:for="{{chatList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['goDetail',['$0'],[[['chatList','',index]]]]]]]}}" class="{{['cu-item',modalName=='move-box-'+index?'move-cur':'']}}" bindtap="__e"><block wx:if="{{item.receive_user_head}}"><view class="cu-avatar round lg" style="{{'background-image:'+('url('+item.receive_user_head+'?imageView2/1/w/200/h/200)')+';'}}"></view></block><block wx:else><view class="cu-avatar round lg" style="{{'background-image:'+('url('+defaultAvator+'?imageView2/1/w/200/h/200)')+';'}}"></view></block><view class="content"><view class="text-grey">{{item.receive_user_name}}</view><view class="text-gray text-sm chat-text-cut">{{''+item.last_content}}</view></view><view class="action"><view class="text-grey text-xs">{{item.last_time}}</view></view></view></block></view></view>