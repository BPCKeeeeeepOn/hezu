<view class="personal-page"><form><view class="cu-form-group padding-top-lg" style="padding-top:70rpx;"><view class="title">姓名</view><input placeholder="请输入真实姓名" name="input" maxlength="50" data-event-opts="{{[['input',[['__set_model',['','user_name','$event',[]]]]]]}}" value="{{user_name}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title">性别</view><radio-group bindinput="__e" class="biaoti" style="flex:1;" name="radio" value="{{sex}}" data-event-opts="{{[['input',[['__set_model',['','sex','$event',[]]]]]]}}"><label><radio style="transform:scale(0.7);" value="男" checked="true" color="#ffa800"></radio>男</label><label><radio style="transform:scale(0.7);" value="女" color="#ffa800"></radio>女</label></radio-group></view><view class="cu-form-group"><view class="title">学校名称</view><input placeholder="请输入大学名称" name="input" maxlength="100" data-event-opts="{{[['input',[['__set_model',['','college','$event',[]]]]]]}}" value="{{college}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title">入学时间</view><picker mode="date" value="{{start_time}}" start="1900-01-01" end="2020-09-01" data-event-opts="{{[['change',[['startDateChange',['$event']]]]]}}" bindchange="__e"><view class="picker">{{''+(start_time||'请选择')+''}}</view></picker></view><view class="cu-form-group"><view class="title">毕业时间</view><picker mode="date" value="{{end_time}}" start="1900-01-01" end="2025-09-01" data-event-opts="{{[['change',[['endDateChange',['$event']]]]]}}" bindchange="__e"><view class="picker">{{''+(end_time||'请选择')+''}}</view></picker></view></form><view class="footer"><button class="footer" style="width:80%;" type="primary" data-event-opts="{{[['tap',[['saveUserInfo',['$event']]]]]}}" bindtap="__e">完成</button></view></view>